<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dekoservidoni.googlefitexample">

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:icon="@mipmap/ic_launcher"
                 android:supportsRtl="true"
                 android:theme="@style/AppTheme">

        <activity
            android:name="com.dekoservidoni.googlefitexample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.dekoservidoni.googlefitexample.HistoryDataActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.dekoservidoni.googlefitexample.CustomDataActivity"
            android:label="@string/app_name" >
        </activity>

        <meta-data android:name="com.google.android.gms.version"
                   android:value="@integer/google_play_services_version" />

    </application>

</manifest>
